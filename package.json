  },
  "devDependencies": {
    "babel-cli": "6",
    "babel-plugin-transform-object-rest-spread": "6",
    "babel-preset-env": "1",
    "babel-register": "6",
    "snazzy": "7",
    "standard": "10"
  },
  "engines": {
    "node": ">=7"
